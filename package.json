{
  "name": "react-simple-dnd-list",
  "description": "A simple drag and drop list component for React",
  "author": {
    "name": "Katsushi OUGI",
    "email": "ougi@to-r.net"
  },
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/react-simple-dnd-list.umd.cjs",
  "module": "./dist/react-simple-dnd-list.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/react-simple-dnd-list.js",
